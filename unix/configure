if eval $ac_link; then
  rm -rf conftest*
  found=yes
else
  rm -rf conftest*
  found=no
fi
rm -f conftest*

	    LDFLAGS=$hold_ldflags
	    echo "$ac_t""$found" 1>&6
	    if test $found = yes; then
